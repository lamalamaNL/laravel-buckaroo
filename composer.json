{
    "name": "lamalamanl/laravel-buckaroo",
    "description": "",
    "keywords": [
        "lamalamanl",
        "laravel-buckaroo-ed"
    ],
    "homepage": "https://github.com/lamalamanl/laravel-buckaroo",
    "license": "MIT",
    "authors": [
        {
            "name": "LamaLama",
            "email": "info@lamalama.nl",
            "homepage": "https://lamalama.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "LamaLama\\LaravelBuckaroo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LamaLama\\LaravelBuckaroo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LamaLama\\LaravelBuckaroo\\BuckarooServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
